{
  "name": "fotopia-serverless",
  "version": "0.1.0",
  "description": "Serverless photo web app using Lambda, S3, DynamoDB",
  "repository": {
    "type": "git",
    "url": "https://github.com/mbudm/fotopia-serverless.git"
  },
  "author": "Steve Roberts <mbudm.com@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "tape -r ts-node/register -r babel-register './fotos/**/*__test__.ts' | tap-spec",
    "test-watch": "tape-watch -r ts-node/register -r babel-register './fotos/**/*__test__.ts' -p tap-spec",
    "functional-prod": "STAGE=prod tape -r ts-node/register -r babel-register ./test/functional.ts | tap-spec",
    "functional-dev": "STAGE=dev tape -r ts-node/register -r babel-register ./test/functional.ts | tap-spec",
    "functional-alpha": "STAGE=alpha tape -r ts-node/register -r babel-register ./test/functional.ts | tap-spec",
    "functional-reset-alpha": "STAGE=alpha tape -r ts-node/register -r babel-register ./test/functional.remoteReset.ts | tap-spec",
    "lint": "eslint 'fotos/**/*.ts' 'test/**/*.ts'"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "@types/sharp": "^0.22.2",
    "amazon-cognito-identity-js": "^3.2.0",
    "babel-runtime": "^6.26.0",
    "bunyan": "^1.8.12",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.0.0",
    "dynamodb-data-types": "^3.0.1",
    "exifreader": "^3.12.0",
    "serverless-domain-manager": "^3.2.6",
    "serverless-s3-remover": "^0.6.0",
    "serverless-stack-output": "^0.2.3",
    "sharp": "0.30.5",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.31",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^12.6.9",
    "@types/tape": "^4.2.33",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "aws-sdk": "2.474.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "es6-promise": "^4.2.8",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "isomorphic-fetch": "^2.2.1",
    "pre-commit": "^1.2.2",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-s3-sync": "^1.8.0",
    "serverless-webpack": "^5.3.1",
    "tap-dot": "^2.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0",
    "tape-watch": "^2.3.0",
    "ts-babel-node": "^1.1.1",
    "tslint": "^5.18.0",
    "webpack": "^4.39.1",
    "webpack-node-externals": "^1.7.2"
  }
}
